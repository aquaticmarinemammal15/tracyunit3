penup()
speed(9)
for i in range(12):
    forward(100)
    pendown()
    begin_fill()
    color("blue")
    circle(10)
    end_fill()
    penup()
    goto(0,0)
    left(10)
    forward(100)
    pendown()
    begin_fill()
    color("red")
    circle(10)
    end_fill()
    penup()
    goto(0,0)
    left(10)
    forward(100)
    pendown()
    begin_fill()
    color("purple")
    circle(10)
    end_fill()
    penup()
    goto(0,0)
    left(10)
